package() {
	echo "SSTRING : The String Manipulation Library"
	mkdir -p "${pkgdir}/usr/include/openutils/sstring"
	cp "${srcdir}/sstring_boolean.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_boolean.hh"
	cp "${srcdir}/sstring_class.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_class.hh"
	cp "${srcdir}/sstring_conversion.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_conversion.hh"
	cp "${srcdir}/sstring_crypto.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_crypto.hh"
	cp "${srcdir}/sstring_cstring.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_cstring.hh"
	cp "${srcdir}/sstring_ctor.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_ctor.hh"
	cp "${srcdir}/sstring_find.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_find.hh"
	cp "${srcdir}/sstring_formatting.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_formatting.hh"
	cp "${srcdir}/sstring.hh" "${pkgdir}/usr/include/openutils/sstring/sstring.hh"
	cp "${srcdir}/sstring_io.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_io.hh"
	cp "${srcdir}/sstring_iterators.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_iterators.hh"
	cp "${srcdir}/sstring_lexer_split.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_lexer_split.hh"
	cp "${srcdir}/sstring_manipulation.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_manipulation.hh"
	cp "${srcdir}/sstring_memory.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_memory.hh"
	cp "${srcdir}/sstring_metrics.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_metrics.hh"
	cp "${srcdir}/sstring_operators.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_operators.hh"
	cp "${srcdir}/sstring_remove.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_remove.hh"
	cp "${srcdir}/sstring_replace.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_replace.hh"
	cp "${srcdir}/sstring_set_get_append.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_set_get_append.hh"
	cp "${srcdir}/sstring_static.hh" "${pkgdir}/usr/include/openutils/sstring/sstring_static.hh"
	echo "Done!"
}
